lumn":17},"source":"components/ConductTransaction.js","original":{"line":54,"column":54}},{"name":"conductTransaction","generated":{"line":124,"column":22},"source":"components/ConductTransaction.js","original":{"line":54,"column":59}},{"generated":{"line":125,"column":0},"source":"components/ConductTransaction.js","original":{"line":54,"column":20}},{"generated":{"line":125,"column":20},"source":"components/ConductTransaction.js","original":{"line":53,"column":16}},{"generated":{"line":125,"column":21},"source":"components/ConductTransaction.js","original":{"line":34,"column":12}},{"generated":{"line":125,"column":22},"source":"components/ConductTransaction.js","original":{"line":33,"column":8}},{"generated":{"line":126,"column":0},"source":"components/ConductTransaction.js","original":{"line":60,"column":5}},{"name":"Component","generated":{"line":130,"column":2},"source":"components/ConductTransaction.js","original":{"line":6,"column":33}},{"generated":{"line":132,"column":0},"source":"components/ConductTransaction.js","original":{"line":61,"column":1}},{"name":"ConductTransaction","generated":{"line":133,"column":15},"source":"components/ConductTransaction.js","original":{"line":63,"column":15}}],"sources":{"components/ConductTransaction.js":"import React, { Component } from 'react';\r\nimport { FormGroup, FormControl, Button } from 'react-bootstrap';\r\nimport { Link } from 'react-router-dom';\r\nimport history from '../history';\r\n\r\nclass ConductTransaction extends Component {\r\n    state = { recipient: '', amount: 0 };\r\n\r\n    updateRecipient = event => {\r\n        this.setState({ recipient: event.target.value });\r\n    }\r\n\r\n    updateAmount = event => {\r\n        this.setState({ amount: Number(event.target.value) });\r\n    }\r\n\r\n    conductTransaction = () => {\r\n        const { recipient, amount } = this.state;\r\n        \r\n        fetch(`${document.location.origin}/api/transact`, {\r\n            method: 'POST',\r\n            headers: { 'Content-Type': 'application/json' },\r\n            body: JSON.stringify({ recipient, amount })}).then(response => response.json()).then(json => {\r\n                alert(json.message || json.type);\r\n                history.push('/transaction-pool');\r\n        });\r\n    }\r\n\r\n\r\n\r\n    render () {\r\n        // debug console.log('this.state', this.state)\r\n        return (\r\n            <div className='ConductTransaction'>\r\n                <Link to='/'>Home</Link>\r\n                <h3>Fazer uma Transação</h3>\r\n                <FormGroup>\r\n                <FormControl\r\n                        input='text'\r\n                        placeholder='recipient'\r\n                        value={this.state.recipient}\r\n                        onChange={this.updateRecipient}\r\n                    />\r\n                </FormGroup>\r\n                <FormGroup>\r\n                <FormControl\r\n                        input='number'\r\n                        placeholder='amount'\r\n                        value={this.state.amount}\r\n                        onChange={this.updateAmount}\r\n                    />\r\n                </FormGroup>\r\n                <div>\r\n                    <Button bsStyle=\"danger\" onClick={this.conductTransaction}>\r\n                        Submeter\r\n                    </Button>\r\n                </div>\r\n            </div>\r\n        )\r\n    }\r\n};\r\n\r\nexport default ConductTransaction;"},"lineCount":null}},"error":null,"hash":"81279dd8f01a9794d76eb6cc85f1196c","cacheData":{"env":{}}}